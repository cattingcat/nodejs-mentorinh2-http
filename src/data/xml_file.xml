<root>
    <mapping name="city">
        <property name="name" index="0"/>
        <property name="timeZoneInfo" index="1" />
        <property name="country" index="2" />
        <property name="coords">
            <property name="x" index="3" />
            <property name="y" index="4" />
        </property>
        <property name="timeZoneName" index="5" />
        <property name="timeZone" property="timeZoneName" relatedTo="timezone" relatedProperty="name" />
    </mapping>

    <mapping name="timezone">
        <property name="code" />
        <property name="name" />
        <property name="gmtOffsetInfo" />
        <property name="offset" />
        <property name="name2" />
        <property name="magicNumbers">
            <property name="num1" />
            <property name="num2" />
            <property name="num3" />
            <property name="num4" />
            <property name="num5" />
            <property name="num6" />
        </property>
        <property name="time" />
        <property name="daylightTimeName" />
        <property name="daylightOffset" />
        <property name="dayloghtMagicNumbers">
            <property name="num1" />
            <property name="num2" />
            <property name="num3" />
            <property name="num4" />
            <property name="num5" />
        </property>
        <property name="daylightTime" />
    </mapping>
</root>
